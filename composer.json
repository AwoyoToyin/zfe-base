{
    "name": "awoyotoyin/zfe-base",
    "type": "library",
    "description": "Base Library for Zend Expressive/Doctrine ORM",
    "version": "1.0.0",
    "keywords": [
        "zend",
        "zend expressive",
        "zend expressive base",
        "zend expressive common",
        "zend expressive abstraction"
    ],
    "homepage": "https://www.github.com/awoyotoyin/zfe-base",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Awoyo Oluwatoyin",
            "email": "awoyotoyin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "dasprid/container-interop-doctrine": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.8 || ^5.7.15"
    },
    "autoload": {
        "psr-4": {
            "Zfe\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zfe\\CommonTest\\": "test/CommonTest/"
        }
    }
}
